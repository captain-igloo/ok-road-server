{% extends 'base.html.twig' %}

{% block stylesheets %}
<link rel="stylesheet" href="/css/okroad.css" />     
<link rel="stylesheet" href="/css/bootstrap.min.css" />
{% endblock %}

{% block javascripts %}
<script>
var token = '{{ csrf_token('authenticate') }}';
